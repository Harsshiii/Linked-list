#include <stdio.h>
#include <string.h>

int main() {
    char students[100][50];  // Array to hold 100 student names
    int count = 0;

    // Inserting student names
    strcpy(students[count++], "Alice");
    strcpy(students[count++], "Bob");
    strcpy(students[count++], "Charlie");

    // Printing the list of students
    for(int i = 0; i < count; i++) {
        printf("Student %d: %s\n", i + 1, students[i]);
    }

    return 0;
}
